<%= form_for [@restaurant, @reservation], :html => {:multipart => true} do |f| %>
	<p><%= f.label :name %></p>
	<p><%= f.text_field :name %></p>
	<p><%= f.label :email %></p>
	<p><%= f.email_field :email %></p>
	<p><%= f.label :requested_datetime, "Reservation Time" %></p>
	<p><%= f.datetime_select :requested_datetime %></p>
	<%= f.submit %>
<% end %>