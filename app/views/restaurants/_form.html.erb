<%= form_for @restaurant, :html => {:multipart => true} do |f| %>
	<p><%= f.label :name %></p>
	<p><%= f.text_field :name %></p>
	<p><%= f.label :description %></p>
	<p><%= f.text_area :description %></p>
	<p><%= f.label :address %></p>
	<p><%= f.text_field :address %></p>
	<p><%= f.label :phone %></p>
	<p><%= f.text_field :phone %></p>
	<p><%= f.file_field :image %></p>
	<%= f.submit %>
<% end %>